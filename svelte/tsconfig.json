{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "allowJs": true,
	  "checkJs": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "module": "esnext",
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "sourceMap": true,
	  "strict": true,
	  "target": "esnext",

	  "types": ["svelte",
		"vite/client"
	  ]
	},
	"include": [
	  "src/**/*.ts",
	  "src/**/*.svelte"
	],
	"exclude": [
	  "node_modules"
	]
  }
  